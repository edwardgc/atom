<rpc xmlns="urn:ietf:params:xml:ns:netconf:base:1.0" message-id="1">
	<edit-config>
		<target>
			<running />
		</target>
		<config>

<classifiers xmlns="urn:bbf:yang:bbf-qos-classifiers">
  <classifier-entry>
    <name>inpbit</name>
    <filter-operation>match-all-filter</filter-operation>
    <match-criteria>
      <tag>
        <index>0</index>
        <in-pbit-list>0-7</in-pbit-list>
      </tag>
			<tag>
				<index>1</index>
				<in-pbit-list>0-7</in-pbit-list>
			</tag>
    </match-criteria>
    <classifier-action-entry-cfg>
      <action-type>pbit-marking</action-type>
      <pbit-marking-cfg>
        <pbit-marking-list>
          <index>0</index>
          <pbit-value>3</pbit-value>
        </pbit-marking-list>
      </pbit-marking-cfg>
    </classifier-action-entry-cfg>
  </classifier-entry>
</classifiers>

<policies xmlns="urn:bbf:yang:bbf-qos-policies">
  <policy>
    <name>my-pbit-remarker-policy</name>
    <classifiers>
      <name>inpbit</name>
    </classifiers>
  </policy>
</policies>
<qos-policy-profiles xmlns="urn:bbf:yang:bbf-qos-policies">
  <policy-profile>
    <name>my-pbit-remarker-profile</name>
    <policy-list>
      <name>my-pbit-remarker-policy</name>
    </policy-list>
  </policy-profile>
</qos-policy-profiles>

<!-- net profile -->
<classifiers xmlns="urn:bbf:yang:bbf-qos-classifiers">
  <classifier-entry>
    <name>inpbit-n</name>
    <filter-operation>match-all-filter</filter-operation>
    <match-criteria>
      <tag>
        <index>0</index>
        <in-pbit-list>0-7</in-pbit-list>
      </tag>
      <tag>
        <index>1</index>
        <in-pbit-list>0-7</in-pbit-list>
      </tag>
    </match-criteria>
		<classifier-action-entry-cfg>
      <action-type>pbit-marking</action-type>
      <pbit-marking-cfg>
        <pbit-marking-list>
          <index>0</index>
          <pbit-from-frame-tag xmlns="http://www.nokia.com/Fixed-Networks/BBA/yang/nokia-sdan-qos-classifier-extension">1</pbit-from-frame-tag>
        </pbit-marking-list>
        <pbit-marking-list>
          <index>1</index>
          <pbit-from-frame-tag xmlns="http://www.nokia.com/Fixed-Networks/BBA/yang/nokia-sdan-qos-classifier-extension">1</pbit-from-frame-tag>
        </pbit-marking-list>
      </pbit-marking-cfg>
    </classifier-action-entry-cfg>
  </classifier-entry>
  <classifier-entry>
    <name>inpbit1-n</name>
    <filter-operation>match-all-filter</filter-operation>
    <match-criteria>
			<tag>
				<index>0</index>
				<in-pbit-list>0-7</in-pbit-list>
			</tag>
    </match-criteria>
    <classifier-action-entry-cfg>
      <action-type>pbit-marking</action-type>
      <pbit-marking-cfg>
        <pbit-marking-list>
          <index>0</index>
          <pbit-value>3</pbit-value>
        </pbit-marking-list>
        <pbit-marking-list>
          <index>1</index>
          <pbit-value>3</pbit-value>
        </pbit-marking-list>
      </pbit-marking-cfg>
    </classifier-action-entry-cfg>
  </classifier-entry>
</classifiers>

<policies xmlns="urn:bbf:yang:bbf-qos-policies">
  <policy>
    <name>my-pbit-remarker-policy-n</name>
    <classifiers>
      <name>inpbit-n</name>
    </classifiers>
    <classifiers>
      <name>inpbit1-n</name>
    </classifiers>
  </policy>
</policies>
<qos-policy-profiles xmlns="urn:bbf:yang:bbf-qos-policies">
  <policy-profile>
    <name>my-pbit-remarker-profile-n</name>
    <policy-list>
      <name>my-pbit-remarker-policy-n</name>
    </policy-list>
  </policy-profile>
</qos-policy-profiles>














</config>
</edit-config>
</rpc>
